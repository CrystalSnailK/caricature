/* pages/recommend/recommend.wxss */
page{
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
  background-color: #f5f5f5;
}
.list_fx{
  /*display: flex;*/
  margin-bottom: 10px;
  width: 100%;
  padding: 5px;
  background-color: #ffffff;
  border-radius: 5px;
  box-sizing: border-box;
  height: 303px;
}
.img{
  background-color: rgba(255, 255, 255, 0);
  width: 100%;
  height: 180px;
}
.list_fx_img{
  border-radius: 10px;
  width: 100%;
  height:100%;
  margin-right: 20px;
}
.fx_list{
width: 100%;
background-color: rgb(255, 255, 255);
margin-top: 5px;
}
.list_name{
  overflow: hidden;
  font-size: 16px;
  font-weight: 550;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  color:rgb(34, 34, 34);
  margin-top: 0px;
  margin-bottom: 5px;
  margin-left: 6px;
}
.list_tags{
background-color: #e9e9e9;
border-radius: 30px;
width: fit-content;
padding: 4.5px;
font-size: 10px;
margin-bottom: 8px;
color: #424242;
}
.fav_img{
height: 14px;
width: 14px;
margin-bottom: 0px;
margin-right: 2px;
background-color: rgb(255, 255, 255);
}
.list_likes{
line-height: 19px;
margin-top: 2px;
background-color: rgb(255, 255, 255);
float: right;
}
.list_des{
  overflow: hidden;
  height: 50px;
  font-size: 10px;
  color: #6b6b6b;
}